{
  "name": "talentan_gatsby_blog",
  "private": true,
  "description": "talentan's personal blog",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby clean && gatsby develop",
    "serve": "gatsby serve",
    "test": "gatsby build && gatsby serve",
    "lint": "eslint --ext .js src && stylelint \"src/**/*.{css,less}\"",
    "deploy": "gh-pages -d public -r $site",
    "push": "git add . && git commit -am 'update' &&  git push origin HEAD",
    "install-action": "npm install @actions/core @actions/exec @actions/github @actions/io shelljs --legacy-peer-deps"
  },
  "dependencies": {
    "antd": "^4.24.15",
    "d3-scale": "^4.0.0",
    "feather-icons-react": "^0.4.1",
    "font-awesome": "^4.7.0",
    "gatsby": "^3.15.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-antd": "^2.2.0",
    "gatsby-plugin-google-gtag": "^3.15.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-less": "5.0",
    "gatsby-plugin-manifest": "^3.15.0",
    "gatsby-plugin-nprogress": "^3.15.0",
    "gatsby-plugin-offline": "^4.15.0",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-robots-txt": "1.3.0",
    "gatsby-plugin-sharp": "^3.15.0",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-valine": "^1.0.2",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "4.0",
    "gatsby-remark-prismjs": "^4.2.0",
    "gatsby-source-filesystem": "^3.15.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-sharp": "^3.15.0",
    "less": "^3.11.1",
    "moment": "^2.30.1",
    "prismjs": "^1.29.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-fontawesome": "^1.7.1",
    "react-grid-layout": "^1.3.0",
    "react-helmet": "^5.2.1",
    "react-pdf": "^4.1.0"
  },
  "devDependencies": {
    "gatsby-plugin-eslint-v2": "2.0.0-beta.0",
    "gh-pages": "3.2.3",
    "husky": "~8.0.3",
    "lint-staged": "15.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/talentAN/talentAN.github.io.git"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
